pipeline {

    node {
       checkout(
            [$class: 'GitSCM',
            branches: [[name: '*/master']],
            doGenerateSubmoduleConfigurations: false,
            extensions: [],
            submoduleCfg: [],
            userRemoteConfigs: [[url: 'https://github.com/Dar005/jenkins/']]]
       )
       echo "From jenkins file!!!....."
    }
}
